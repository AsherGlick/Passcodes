{
	"manifest_version":2,

	"name":"Passcodes",
	"description": "Securely create passwords for different accounts",
	"version": "1.0",

	"permissions": ["contextMenus"],
 "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
		"scripts": ["script.js"]
	},
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	}
}